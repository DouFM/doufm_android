<?xml version="1.0" encoding="utf-8"?>

<set xmlns:android="http://schemas.android.com/apk/res/android">
    <rotate
        android:fromDegrees="30"
        android:toDegrees="0"
        android:duration="300"
        android:pivotX="0%"
        android:pivotY="0%" />
    <rotate
        android:fromDegrees="-30"
        android:toDegrees="0"
        android:duration="300"
        android:startOffset="300"
        android:pivotX="0%"
        android:pivotY="0%" />
</set>